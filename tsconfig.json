{
  "compilerOptions": {
    "types": ["cypress", "cypress-map"],
    "target": "es2016",
    "strict": true,
    "allowJs": true,
    "noEmit": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["cypress/support/*.ts" ,"cypress/e2e/*.ts"]
}